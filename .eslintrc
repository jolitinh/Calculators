{
  "env": {
    "es6": true,
    "commonjs": true,
    "browser": true,
    "node": true
  },
  "globals": {
    "BigNumber": true,
    "angular": true
  },
  "rules": {
    "no-bitwise": 2,
    "curly": [
      2,
      "all"
    ],
    "eqeqeq": 2,
    "wrap-iife": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 2,
    "new-cap": 2,
    "no-caller": 2,
    "quotes": [
      2,
      "single",
      "avoid-escape"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "strict": [
      2,
      "function"
    ],
    "no-loop-func": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "no-empty": 2,
    "array-bracket-spacing": [
      2,
      "never",
      {
        "singleValue": true
      }
    ],
    "space-in-parens": [
      2,
      "never"
    ],
    "quote-props": [
      2,
      "as-needed"
    ],
    "key-spacing": [
      2,
      {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "space-unary-ops": [
      2,
      {
        "words": false,
        "nonwords": false
      }
    ],
    "no-mixed-spaces-and-tabs": 2,
    "no-trailing-spaces": 2,
    "comma-dangle": [
      2,
      "never"
    ],
    "yoda": [
      2,
      "never"
    ],
    "no-with": 2,
    "no-multiple-empty-lines": 2,
    "space-before-blocks": [
      2,
      "always"
    ],
    "comma-style": [
      2,
      "last"
    ],
    "space-infix-ops": 2,
    "camelcase": [
      2,
      {
        "properties": "always"
      }
    ],
    "dot-notation": 2,
    "keyword-spacing": [
      2,
      {
        "overrides": {
          "if": {
            "before": true
          },
          "else": {
            "before": true
          },
          "for": {
            "before": true
          },
          "try": {
            "before": true
          },
          "catch": {
            "before": true
          },
          "finally": {
            "before": true
          },
          "break": {
            "before": true
          },
          "do": {
            "before": true
          },
          "while": {
            "before": true
          },
          "switch": {
            "before": true
          }
        }
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "func-style": 0,
    "brace-style": 2,
    "no-multi-str": 2,
    "one-var": [
      2,
      "never"
    ],
    "operator-linebreak": [
      2,
      "before"
    ],
    "no-spaced-func": 2,
    "newline-after-var": [
      2,
      "always"
    ],
    "eol-last": 2,
    "semi": [
      2,
      "always"
    ]
  }
}
